<script id="searchTemplate" type="text/x-underscore">
	<% function renderItemLine(item, iterator, path){ %>
    <% var url = MG.Config.page === 'map' ? item.url.replace('weather','nowcast') : item.url; %>
    <li>
        <a href="<%= url %><%= path %>" class="nolink black found_link js-li js-a" data-index="<%= iterator %>" data-type="city">
            <i class="fa flags flag-<% if(item.countryCode){%><%= item.countryCode.toLowerCase()%><%}else{%>empty<%}%>"></i>
            <span class="black found_city"><% if(item.type === 'airport'){%>
                    <i class="found_airport">
                        <svg viewBox="0 0 52 48"><path d="M31 21l-17-20.983h-4.686l10.686 20.983h-13l-5-9h-2l3 12-3 12h2l5-9h13l-10.686 20.983h4.686l17-20.983h17c2.151 0 4-.846 4-3 0-2.151-1.849-3-4-3h-17z"/></svg>
                    </i>
                <%}%><%= item.name %></span>
            <span class="gray found_location">
                <% if(item.subDistrictName) {%><%= item.subDistrictName%>,&nbsp;<%}%><% if(item.districtName) {%><%= item.districtName%>,&nbsp;<%}%><%= item.countryName%>
            </span>
        </a>
    </li>
<%}%>
<% function renderMoreLine(iterator, search){%>
    <li>
        <a href="/search/<%= search %>/" class="js-li js-a nolink blue found_link found_all" data-index="<%= iterator %>" data-type="more">
            Найти все пункты
        </a>
    </li>
<%}%>
<% if(lastViewedCities.total){
    var iterator = 0;
    _.forEach(lastViewedCities.items, function(item, i){
        renderItemLine(item, iterator, path);
        iterator++;
    });
}%>
<% if(searchedCities.items.length){
    var iterator = 0;
    _.forEach(searchedCities.items, function(item, i){
        renderItemLine(item, iterator, path);
        iterator++;
    });
    if(searchedCities.more){
        renderMoreLine(iterator, searchedCities.search);
    }
} %>

<% if(searchedCities.error || (searchedCities.items.length == 0 && searchedCities.search.length >= searchedCities.minChars)){%>
    <li class="found_link found_empty">
        <span class="found_text bold">По вашему запросу ничего не найдено</span>
    </li>
<% } %>
</script>
