<script id="favoritesTemplate" type="text/x-underscore">
    <% if(total != 0){ %>
    <%
        var page = MG.Helper.getPath();
        _.forEach(_.sortBy(items, 'name'), function(item,i){
            var weatherBorder = MG.Helper.color(Math.round(item.cw.temperature.air), 1);
            var weatherBg = MG.Helper.color(Math.round(item.cw.temperature.air), MG.Config.bgTempOpacity);
            var labelValue = $('.mylocations').hasClass('edit') ? item.id : '';
            var url = MG.Config.page === 'map' ? item.url.replace('weather','nowcast') : item.url;
        %>
        <a href="<%= url %><%= page %>" class="mylocations_item black nolink no_border js-myloc-uid" data-uid="<%= item.id %>" data-timezone="<%= item.city ? item.city.date.offset : ''%>">
            <label class="mylocations_select_label" for="id-<%= labelValue %>" data-id="<%= item.id %>">
                <div class="mylocations_section mylocations_select">
                    <input type="checkbox" class="mylocations_checkbox" data-id="<%= item.id %>" id="id-<%= item.id %>">
                </div>
                <div class="mylocations_section mylocations_img">
                    <img src="/assets/flat-ui/img/icons/weather/flat/svg/<%= item.cw.icon %>.svg" class="mylocations_icon js-myloc-img">
                </div>
                <div class="mylocations_section mylocations_place">
                    <div class="mylocations_name"><%= item.name %></div>
                    <div class="mylocations_geo gray js-myloc-geo"><%= item.city ? item.city.str : '' %></div>
                    <div class="mylocations_time gray js-myloc-time">&nbsp;</div>
                </div>
                <div class="mylocations_section mylocations_weather">
                    <span class="mylocations_degrees" style="border-bottom:1px solid <%= weatherBorder %>; background-color: <%= weatherBg %>">
                        <span class="mylocations_degrees_val js-myloc-temp"><%= pureTemp(Math.round(item.cw.temperature.air)) %></span>
                    </span>
                </div>
            </label>
        </a>
    <% }); %>
    <div class="mylocations_block">
        <div class="mylocations_btn _edit" id="fav_edit">
            <span class="mylocations_btn_txt blue">Изменить</span>
        </div>
        <div class="mylocations_btn __disabled _del" id="fav_del">
            <span class="mylocations_btn_txt weekend">Удалить</span>
        </div>
        <div class="mylocations_btn _ok" id="fav_ok">
            <span class="mylocations_btn_txt blue">Готово</span>
        </div>
    </div>
<% } %>
